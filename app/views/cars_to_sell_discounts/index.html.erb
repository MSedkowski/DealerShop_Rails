<div class="main-field">
  <div class="row header-row">
    <h3 class="col s6 offset-s3" id="supply-list-header">Zniżki</h3>
  </div>
  <div class="row">
    <table class="striped col l10 offset-l1">
      <thead>
    <tr>
      <th>Auto</th>
      <th>Zniżka</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars_to_sell_discounts.each do |cars_to_sell_discount| %>
      <tr>
        <td><%= cars_to_sell_discount.car %></td>
        <td><%= cars_to_sell_discount.discount %></td>
        <td><%= link_to 'Pokaż', cars_to_sell_discount %></td>
        <td><%= link_to 'Edytuj', edit_cars_to_sell_discount_path(cars_to_sell_discount) %></td>
        <td><%= link_to 'Usuń', cars_to_sell_discount, method: :delete, data: { confirm: 'Czy jesteś pewien?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
  </div>
</div>