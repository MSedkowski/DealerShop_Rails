<p id="notice"><%= notice %></p>

<h1>Cars To Sell Discounts</h1>

<table>
  <thead>
    <tr>
      <th>Car</th>
      <th>Discount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars_to_sell_discounts.each do |cars_to_sell_discount| %>
      <tr>
        <td><%= cars_to_sell_discount.car %></td>
        <td><%= cars_to_sell_discount.discount %></td>
        <td><%= link_to 'Show', cars_to_sell_discount %></td>
        <td><%= link_to 'Edit', edit_cars_to_sell_discount_path(cars_to_sell_discount) %></td>
        <td><%= link_to 'Destroy', cars_to_sell_discount, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cars To Sell Discount', new_cars_to_sell_discount_path %>
