<div class="main-field">
  <div class="row header-row">
    <h3 class="col s6 offset-s3" id="supply-list-header">Auta w wypożyczalni</h3>
  </div>
  <div class="row">
    <table class="striped col l10 offset-l1">
      <thead>
      <tr>
        <th>Marka</th>
        <th>Model</th>
        <th>Dodatkowe informację</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
      <% @rental_cars.each do |rental_car| %>
        <tr>
          <td><%= rental_car.brand %></td>
          <td><%= rental_car.model %></td>
          <td><%= rental_car.additional_information %></td>
          <td><%= link_to 'Pokaż', rental_car %></td>
          <td><%= link_to 'Edytuj', edit_rental_car_path(rental_car) %></td>
          <td><%= link_to 'Usuń', rental_car, method: :delete, data: { confirm: 'Czy jesteś pewien?' } %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
