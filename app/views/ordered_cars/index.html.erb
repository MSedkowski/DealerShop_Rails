<div class="main-field">
  <div class="row header-row">
    <h3 class="col s6 offset-s3" id="supply-list-header">Zamówione auta</h3>
  </div>
  <div class="row">
    <table class="striped col l10 offset-l1">
      <thead>
      <tr>
      <th>Auto</th>
      <th>Klient</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordered_cars.each do |ordered_car| %>
      <tr>
        <td><%= ordered_car.car %></td>
        <td><%= ordered_car.client %></td>
        <td><%= ordered_car.status %></td>
        <td><%= link_to 'Pokaż', ordered_car %></td>
        <td><%= link_to 'Edytuj', edit_ordered_car_path(ordered_car) %></td>
        <td><%= link_to 'Usuń', ordered_car, method: :delete, data: { confirm: 'Czy jesteś pewien?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
  </div>
</div>