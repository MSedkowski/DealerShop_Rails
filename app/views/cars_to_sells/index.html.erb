<p id="notice"><%= notice %></p>

<h1>Cars To Sells</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>Price</th>
      <th>Additional information</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cars_to_sells.each do |cars_to_sell| %>
      <tr>
        <td><%= cars_to_sell.brand %></td>
        <td><%= cars_to_sell.model %></td>
        <td><%= cars_to_sell.price %></td>
        <td><%= cars_to_sell.additional_information %></td>
        <td><%= link_to 'Show', cars_to_sell %></td>
        <td><%= link_to 'Edit', edit_cars_to_sell_path(cars_to_sell) %></td>
        <td><%= link_to 'Destroy', cars_to_sell, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cars To Sell', new_cars_to_sell_path %>
